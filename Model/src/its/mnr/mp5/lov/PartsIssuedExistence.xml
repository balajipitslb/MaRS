      <Attr Name="_DisplaySize" Value="18"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="its.mnr.mp5.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
