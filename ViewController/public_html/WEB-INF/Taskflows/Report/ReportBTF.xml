<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="ReportBTF">
    <template-reference>
      <document>/WEB-INF/error-handler-flow.xml</document>
      <id>error-handler-flow</id>
    </template-reference>
    <default-activity>report</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>ModuleNavBean</managed-bean-name>
      <managed-bean-class>its.mnr.mp5.view.ModuleNavBean</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="report">
      <page>/Content/Report/report.jspx</page>
    </view>
    <task-flow-return id="Exit">
      <outcome>
        <name>Exit</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__2">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>exit</from-outcome>
        <to-activity-id>Exit</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
